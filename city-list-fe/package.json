{
  "name": "city-list-fe",
  "version": "0.1.0",
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "bootstrap": "4.5.3",
    "connected-react-router": "6.7.0",
    "deepmerge": "4.2.2",
    "jest-environment-jsdom-sixteen": "2.0.0",
    "js-cookie": "2.2.1",
    "ramda": "0.27.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-bootstrap": "^2.7.0",
    "react-intl": "3.12.1",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-table": "7.8.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0"
  },
  "publishConfig": {
    "proxy": null,
    "https-proxy": null
  },
  "files": [
    "build"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test-watchall": "react-scripts test --watchAll=true",
    "test-coverage": "react-scripts test --coverage --watchAll=false",
    "ci-install": "npm install",
    "eject": "react-scripts eject",
    "publish-nexus": "npm publish",
    "pack": "npm pack",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "@babel/core": "7.8.4",
    "@storybook/addon-info": "5.3.13",
    "@storybook/addon-knobs": "5.3.13",
    "@storybook/addons": "5.3.13",
    "@storybook/react": "5.3.13",
    "@testing-library/react": "11.0.2",
    "@testing-library/user-event": "12.1.4",
    "node-sass": "4.13.1",
    "raw-loader": "4.0.0",
    "sass-loader": "8.0.2",
    "storybook": "5.3.13"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!**/ci/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "presets": [
    "es2015",
    "react"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "main": "index.js"
}
